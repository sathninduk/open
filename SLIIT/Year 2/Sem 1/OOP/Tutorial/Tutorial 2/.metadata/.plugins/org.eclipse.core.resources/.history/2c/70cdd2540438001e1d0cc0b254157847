import ex2.Point3D;
import java.util.Scanner;

public class Ex2 {
	public static void main(String args[]) {
		// scanner object
		Scanner sc = new Scanner(System.in);
		
		// object creation
		Point3D p1 = new Point3D(1, 2, 3);
		Point3D p2 = new Point3D(3, 1, 5);
		
		// output
		System.out.println("Object 1 distance: " + p1.distance());
		System.out.println("Object 2 distance: " + p2.distance());
		
	}
}
